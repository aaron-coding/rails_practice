<h1>Log in</h1>

<form action="/sessions" method="POST">
  <label for="user_email">Email:</label>
  <input 
    type="text" 
    id="user_email" 
    name="user[email]"><br>
    
  <label for="user_password">Password:</label>
  <input 
    type="password" 
    id="user_password" 
    name="user[password]">  
    
  <input type="submit" value="Log in">
  
</form>
