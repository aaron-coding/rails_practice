<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag), 
      class: 'tag_title' %> <%= link_to "delete tag", tag_path(tag), style: 'padding-left: 25px', method: :delete, data: {confirm: "Really delete this tag?"} %>
    </li>
    <% end %>
  </ul>
  <ul>
    <li>
    <%= link_to "Back to Articles", articles_path%>
  </li>
  </ul>

  